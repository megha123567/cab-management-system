<div class="container">
    <h1 class="text-center">Booking </h1>
    <div class="row">
        <div class="col-sm-6 offset-sm-3">
            <form method="post">
                <div>
                    <label for="dateofbooking">Date of Booking</label>
                    <input type="date" name="dateofbooking" id="dateofbooking" class="form-control">
                </div>
                <div>
                     <label for="cabfrom">Cab from</label>
                     <select name="cabfrom" id="cabfrom" class="form-control">
                        {{!-- <option value="place1">place1</option>
                        <option value="place2">place2</option>
                        <option value="place3">place3</option> --}}
                        {{#each pickup}}
                        <option value="{{this.dataValues.cab_from}}">{{this.dataValues.cab_from}}</option>
                        {{/each}}
                     </select>
                </div>
                <div>
                    <label for="cabto">Cab To</label>
                     <select name="cabto" id="cabto" class="form-control">
                        {{!-- <option value="place6">place6</option>
                        <option value="place7">place7</option>
                        <option value="place8">place8</option> --}}
                        {{#each cabto}}
                        <option value={{this.dataValues.cab_to}}>{{this.dataValues.cab_to}}</option>
                        {{/each}}
                     </select>
                    
                </div>
                <div>
                    <label for="bookingtime">Booking Time</label>
                    <input type="time" name="bookingtime" id="bookingtime" class="form-control">
                </div>
                <div class="d-grid mt-4">
                    <button type="submit" class="btn btn-outline-info shadow">Book Now</button>
                </div>
            </form>
        </div>
    </div>
</div>