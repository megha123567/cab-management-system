<pre>


</pre>
<div class="col-sm-5 offset-sm-4">
    <div class=" card w-75 text-center">
        <div class="card-header">
            <h4 class="card-title">Your Profile</h4>
        </div>
        <div class="card-body">

            <table class="table">
                <tr>
                    <th>Name</th>
                    <td>{{data.fisrtName}} {{data.lastName}}</td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td>{{data.email}}</td>
                </tr>
                <tr>
                    <th>Mobile</th>
                    <td>{{data.mobile}}</td>
                </tr>
                <tr>
                    <th>Date Of Birth</th>
                    <td>{{data.dob}}</td>
                </tr>
            </table>
            <div>
                <button class="btn btn-outline-info"><a href="/update/{{data.passenger_id}}">Update your
                        profile</a></button>
                <button class="btn btn-outline-info"><a href="/delete/{{data.passenger_id}}"
                        onclick="return confirm('Are you sure you want to delete this') ">Delete your
                        profile</a></button>
            </div>
            <pre></pre>
            {{#if data.role}}
            <div>
                <button class="btn btn-outline-info"><a href="/">View all passengers</a></button>
                <button class="btn btn-outline-info"><a href="/booking">View all booking</a></button>
            </div>
            {{/if}}
        </div>
    </div>
</div>